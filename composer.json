{
    "name": "spatie/eloquent-sortable",
    "description": "Sortable behaviour for eloquent models",
    "homepage": "https://github.com/spatie/eloquent-sortable",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be"
        }
    ],
    "keywords":
        [
            "sort",
            "sortable",
            "eloquent",
            "model",
            "laravel",
            "behaviour"
        ],
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "laravel/framework": "~5.8.0|^6.0|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "^7.5",
        "orchestra/testbench": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\EloquentSortable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\EloquentSortable\\Test\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
